    {
        "id": "2b2954fbd0ef2b41",
        "type": "function",
        "z": "c1d30ccfc00f7025",
        "name": "showData",
        "func": "var data = msg.payload;\n\nlet prodPerMin = { payload: data.prodPerMin, topic: \"prodPerMin\" };\nlet prodPerHr = { payload: data.prodPerHr, topic: \"prodPerHr\" };\nlet prodPerDay = { payload: data.prodPerDay, topic: \"prodPerDay\" };\n\nlet temp0 = { payload: data.temp[0], topic: \"temp0\" };\nlet temp1 = { payload: data.temp[1], topic: \"temp1\" }; \nlet temp2 = { payload: data.temp[2], topic: \"temp2\" }; \n\nlet hum0 = { payload: data.hum[0], topic: \"hum0\" };\nlet hum1 = { payload: data.hum[1], topic: \"hum1\" };\nlet hum2 = { payload: data.hum[2], topic: \"hum2\" };\n\nlet pres0 = { payload: data.pres[0], topic: \"pres0\" };\nlet pres1 = { payload: data.pres[1], topic: \"pres1\" };\nlet pres2 = { payload: data.pres[2], topic: \"pres2\" };\n\nlet status = { payload: \"Stopped\", topic: \"belt\"};\nif (data.belt) {\n    status.payload = \"Running\";\n}\n\nlet products = { payload: data.products, topic: \"Total Products\" };\n\nreturn [prodPerMin, prodPerHr, prodPerDay, temp0, temp1, temp2, hum0, hum1, hum2, pres0, pres1, pres2, status, products];",
